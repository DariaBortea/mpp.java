package eu.ase.oop;

import java.security.cert.CertificateEncodingException;

public class certificate {
    private int id;
    private String name; //string e cu s mare pt ca string este o clasa

    public certificate(int _id, String _name)
    {
        this.id=_id;
        this.name=_name;
    }

    public int getId()
    {
        return this.id;
    }

    public String getName(){
        return this.name;
    }

    public void setId(int id){
        this.id=id;
    }

    public  void setName(String name){
        this.name=name;
    }

    public certificate myClone(){
        certificate newCert=new certificate (this.id, this.name);
        return newCert ;
    }
//override la meth de == initiala(mai multe detalii in main)
    @Override
    public boolean equals(Object obj){

        certificate c= (certificate) obj;
        if(this.id==c.id && this.name.equals(c.name)){
           return true;
        }
        return false;
    }
}
